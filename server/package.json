{
  "name": "server",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "start": "ts-node --esm src/main.ts",
    "watch": "ts-node-dev --esm src/main.ts",
    "build": "tsc",
    "clean": "rm -Rf ./dist",
    "up": "npm-check-updates -u && npm install && npm dedupe && npm audit fix",
    "test": "jest"
  },
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js"
    },
    "./protocol": {
      "types": "./dist/lib/protocol.d.ts",
      "import": "./dist/lib/protocol.js"
    }
  },
  "types": "./dist/main.d.ts",
  "main": "./dist/main.js",
  "author": "Kaede Fujisaki",
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "@types/generic-pool": "^3.1.10",
    "@types/jest": "^27.4.1",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.23",
    "esm": "^3.2.25",
    "jest": "^27.5.1",
    "npm-check-updates": "^12.5.4",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8"
  },
  "dependencies": {
    "@expo/spawn-async": "^1.6.0",
    "@wordpress/escape-html": "^2.5.0",
    "axios": "^0.26.1",
    "dayjs": "^1.11.1",
    "exifr": "^7.1.3",
    "fastify": "^3.27.4",
    "fastify-static": "^4.6.1",
    "file-type": "^17.1.1",
    "generic-pool": "^3.8.2",
    "handlebars": "^4.7.7",
    "lib": "file:../lib",
    "marked": "^4.0.12",
    "node-cache": "^5.1.2",
    "pino": "^7.9.2",
    "ts-node": "^10.7.0",
    "ts-postgres": "^1.2.1",
    "typescript": "^4.6.3"
  }
}
