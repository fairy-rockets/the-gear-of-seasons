{
  "name": "lib",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "clean": "rm -Rf ./dist",
    "up": "npm-check-updates -u && npm install && npm dedupe && npm audit fix",
    "test": "jest"
  },
  "main": "dist/lib.js",
  "types": "dist/lib.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "imports": "./dist/lib.js"
    },
    "./asset": {
      "types": "./dist/asset.d.ts",
      "import": "./dist/asset.js"
    },
    "./fml": {
      "types": "./dist/fml.d.ts",
      "import": "./dist/fml.js"
    },
    "./media/probe": {
      "type": "./dist/media/probe.d.ts",
      "import": "./dist/media/probe.js"
    },
    "./media/convert": {
      "types": "./dist/media/convert.d.ts",
      "import": "./dist/media/convert.js"
    },
    "./protocol": {
      "types": "./dist/protocol.d.ts",
      "import": "./dist/protocol.js"
    },
    "./md5sum": {
      "types": "./dist/md5sum.d.ts",
      "import": "./dist/md5sum.js"
    }
  },
  "typesVersions": {
    "*": {
      "asset": [
        "dist/asset"
      ],
      "fml": [
        "dist/fml"
      ],
      "media/probe": [
        "dist/media/probe"
      ],
      "media/convert": [
        "dist/media/convert"
      ],
      "protocol": [
        "dist/protocol"
      ],
      "md5sum": [
        "dist/md5sum"
      ]
    }
  },
  "author": "Kaede Fujisaki",
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "jest": "^27.5.1",
    "npm-check-updates": "^12.5.4",
    "ts-jest": "^27.1.4"
  },
  "dependencies": {
    "@expo/spawn-async": "^1.6.0",
    "dayjs": "^1.11.0",
    "exifr": "^7.1.3",
    "file-type": "^17.1.1",
    "typescript": "^4.6.3"
  }
}
